<window id="12">
	<defaultcontrol>3</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<controls>
		<control type="group">
			<include>CommonSettingsBackground</include>
		</control>
		<control type="group">
			<visible>Window.Previous(Weather) | [Window.Previous(Home) + Skin.String(SettingsPrevious,Weather)]</visible>
			<include>CommonWeatherBackground</include>
		</control>
		<control type="group">
			<visible>Window.Previous(Programs) | [Window.Previous(Home) + Skin.String(SettingsPrevious,Programs)]</visible>
			<include>CommonProgramsBackground</include>
		</control>
		<control type="group">
			<visible>Window.Previous(Pictures) | [Window.Previous(Home) + Skin.String(SettingsPrevious,Pictures)]</visible>
			<include>CommonPicturesBackground</include>
		</control>
		<control type="group">
			<visible>Window.Previous(MusicFiles)  | Window.Previous(MusicLibrary) | [Window.Previous(Home) + Skin.String(SettingsPrevious,Music)]</visible>
			<include>CommonMusicBackground</include>
		</control>
		<control type="group">
			<visible>Window.Previous(VideoFiles) | Window.Previous(VideoLibrary) | [Window.Previous(Home) + Skin.String(SettingsPrevious,Videos)]</visible>
			<include>CommonVideoBackground</include>
		</control>
		<include>CommonNowPlaying</include>
		<control type="group">
			<include>Window_OpenClose_Animation</include>
			<control type="image">
				<posx>0</posx>
				<posy>100</posy>
				<width>1280</width>
				<height>520</height>
				<texture>black-back.png</texture>
			</control>
			<control type="label" id="2">
				<description>Fake Label so we can pass it value down to the one below</description>
				<posx>0</posx>
				<posy>0</posy>
				<width>0</width>
				<height>0</height>
				<font>-</font>
				<visible>false</visible>
\			</control>
			<control type="label">
				<posx>285</posx>
				<posy>115</posy>
				<width>705</width>
				<height>30</height>
				<font>font18</font>
				<textcolor>white</textcolor>
				<scroll>true</scroll>
				<align>center</align>
				<aligny>center</aligny>
				<label>[UPPERCASE]$LOCALIZE[5] - $INFO[Control.GetLabel(2)][/UPPERCASE]</label>
			</control>
			<control type="image">
				<posx>285</posx>
				<posy>150</posy>
				<width>705</width>
				<height>8</height>
				<texture>StreakBlueFull.png</texture>
			</control>
			<control type="grouplist" id="3">
				<description>button area</description>
				<posx>100</posx>
				<posy>170</posy>
				<width>200</width>
				<height>440</height>
				<itemgap>5</itemgap>
				<onleft>5</onleft>
				<onright>5</onright>
				<onup>3</onup>
				<ondown>3</ondown>
			</control>
			<control type="grouplist" id="5">
				<description>control area</description>
				<posx>350</posx>
				<posy>170</posy>
				<width>820</width>
				<height>440</height>
				<itemgap>5</itemgap>
				<pagecontrol>-</pagecontrol>
				<onleft>3</onleft>
				<onright>90</onright>
				<onup>5</onup>
				<ondown>5</ondown>
			</control>
		</control>
		<control type="button" id="10">
			<description>Default Category Button</description>
			<posx>0</posx>
			<posy>0</posy>
			<height>30</height>
			<width>200</width>
			<label>-</label>
			<align>center</align>
			<font>font12</font>
			<textcolor>grey2</textcolor>
			<focusedcolor>white</focusedcolor>
			<texturefocus>list-focus.png</texturefocus>
			<texturenofocus>-</texturenofocus>
		</control>
		<control type="button" id="7">
			<description>Default Button</description>
			<posx>0</posx>
			<posy>0</posy>
			<height>30</height>
			<font>font12</font>
			<textcolor>grey2</textcolor>
			<focusedcolor>white</focusedcolor>
			<texturefocus>list-focus.png</texturefocus>
			<texturenofocus>-</texturenofocus>
		</control>
		<control type="radiobutton" id="8">
			<description>Default RadioButton</description>
			<posx>0</posx>
			<posy>0</posy>
			<height>30</height>
			<font>font12</font>
			<textcolor>grey2</textcolor>
			<focusedcolor>white</focusedcolor>
			<texturefocus>list-focus.png</texturefocus>
			<texturenofocus>-</texturenofocus>
		</control>
		<control type="spincontrolex" id="9">
			<description>Default SpinControlex</description>
			<posx>0</posx>
			<posy>0</posy>
			<height>30</height>
			<font>font12</font>
			<textcolor>grey2</textcolor>
			<focusedcolor>white</focusedcolor>
			<texturefocus>list-focus.png</texturefocus>
			<texturenofocus>-</texturenofocus>
			<aligny>center</aligny>
			<reverse>yes</reverse>
		</control>
		<include>CommonSideBlades_NonMedia</include>
	</controls>
</window>
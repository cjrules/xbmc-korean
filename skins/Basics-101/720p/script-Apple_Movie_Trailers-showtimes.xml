					<font>font12</font>
					<textcolor>grey</textcolor>
					<aligny>center</aligny>
					<align>left</align>
					<info>ListItem.Label2</info>
				</control>
			</itemlayout>
			<focusedlayout height="100">
				<control type="image">
					<width>770</width>
					<height>95</height>
					<posx>0</posx>
					<posy>0</posy>

			<font>font12</font>
			<label>-</label>
			<textcolor>red</textcolor>
			<shadowcolor>black</shadowcolor>
		</control>
		<!-- ** Required ** Do not change <id> or <type> -->
		<control type="list" id="100">
			<description>Team Credits List</description>
			<posx>15</posx>
			<posy>100</posy>
			<width>770</width>
			<height>400</height>
			<scrolltime>200</scrolltime>
			<onup>100</onup>
			<ondown>100</ondown>
			<itemlayout height="100">
				<control type="image">
					<width>770</width>
					<height>95</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture border="5">button-nofocus.png</texture>
				</control>
				<control type="label">
					<posx>10</posx>
					<posy>10</posy>
					<width>750</width>
					<height>20</height>
					<font>font12</font>
					<aligny>center</aligny>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="label">
					<posx>750</posx>
					<posy>10</posy>
					<width>700</width>
					<height>20</height>
					<font>font12</font>

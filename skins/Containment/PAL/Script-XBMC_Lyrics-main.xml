<window>
	<allowoverlay>no</allowoverlay>
	<defaultcontrol always="false">101</defaultcontrol>
	<coordinates>
		<system>1</system>
		<posx>0</posx>
		<posy>0</posy>
	</coordinates>
	<controls>
		<!-- Start Menu Background -->
		<control type="image">
			<description>background top image</description>
			<posx>0</posx>
			<posy>40</posy>
			<width>480</width>
			<height>30</height>
			<colordiffuse>FFFFFFFF</colordiffuse>
			<texture>dialog-context-end.png</texture>
		</control>
		<control type="image">
			<description>background image</description>
			<posx>0</posx>
			<posy>70</posy>
			<width>480</width>
			<height>390</height>
			<colordiffuse>FFFFFFFF</colordiffuse>
			<texture>dialog-context-middle.png</texture>
		</control>
		<control type="image">
			<description>background bottom image</description>
			<posx>0</posx>
			<posy>460</posy>
			<width>480</width>
			<height>30</height>
			<colordiffuse>FFFFFFFF</colordiffuse>
			<texture flipy="true">dialog-context-end.png</texture>
		</control>
		<control type="image">
			<description>separator</description>
			<posx>5</posx>
			<posy>155</posy>
			<width>470</width>
			<height>5</height>
			<colordiffuse>object-color</colordiffuse>
			<texture>line-horizontal.png</texture>
		</control>
		<!-- End Menu Background -->
		<!-- ** Required ** Do not change <id> or <type> -->
		<control type="textbox" id="100">
			<description>textarea</description>
			<posx>20</posx>
			<posy>165</posy>
			<width>410</width>
			<height>310</height>
			<onup>101</onup>
			<ondown>101</ondown>
			<onleft>101</onleft>
			<onright>101</onright>
			<pagecontrol>101</pagecontrol>
			<font>font12</font>
			<textcolor>button-nofocus-text</textcolor>
		</control>
		<control type="spincontrol" id="101">
			<description>Next page button</description>
			<posx>430</posx>
			<posy>455</posy>
			<subtype>page</subtype>
			<font>font12</font>
			<onleft>101</onleft>
			<onright>101</onright>
			<ondown>101</ondown>
			<onup>101</onup>
			<showonepage>false</showonepage>
			<visible>Control.IsVisible(100)</visible>
		</control>
		<!-- ** Required ** Do not change <id> or <type> -->
		<control type="list" id="110">
			<posx>20</posx>
			<posy>165</posy>
			<width>410</width>
			<height>310</height>
			<onleft>111</onleft>
			<onright>111</onright>
			<onup>110</onup>
			<ondown>110</ondown>
			<viewtype label="535">list</viewtype>
			<pagecontrol>111</pagecontrol>
			<scrolltime>200</scrolltime>
			<include>contentpanelslide</include>
			<itemlayout height="32">
				<control type="label">
					<posx>0</posx>
					<posy>0</posy>
					<width>400</width>
					<height>32</height>
					<font>font12</font>
					<aligny>center</aligny>
					<textcolor>button-nofocus-text</textcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			<focusedlayout height="32">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>410</width>
					<height>32</height>
					<visible>Control.HasFocus(110)</visible>
					<colordiffuse>object-color</colordiffuse>
					<texture>line.png</texture>
				</control>
				<control type="label">
					<posx>0</posx>
					<posy>0</posy>
					<width>400</width>
					<height>32</height>
					<font>font12</font>
					<aligny>center</aligny>
					<textcolor>main-text</textcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
			</focusedlayout>
		</control>
		<control type="spincontrol" id="111">
			<description>Next page button</description>
			<posx>430</posx>
			<posy>455</posy>
			<subtype>page</subtype>
			<font>font12</font>
			<onleft>110</onleft>
			<onright>110</onright>
			<ondown>110</ondown>
			<onup>110</onup>
			<showonepage>false</showonepage>
			<visible>Control.IsVisible(110)</visible>
		</control>
		<!-- ** Required ** Do not change <id> or <type> -->
		<control type="list" id="120">
			<posx>20</posx>
			<posy>165</posy>
			<width>410</width>
			<height>310</height>
			<onleft>121</onleft>
			<onright>121</onright>
			<onup>120</onup>
			<ondown>120</ondown>
			<viewtype label="535">list</viewtype>
			<pagecontrol>121</pagecontrol>
			<scrolltime>200</scrolltime>
			<include>contentpanelslide</include>
			<itemlayout height="32">
				<control type="label">
					<posx>0</posx>
					<posy>0</posy>
					<width>400</width>
					<height>32</height>
					<font>font12</font>
					<aligny>center</aligny>
					<textcolor>button-nofocus-text</textcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			<focusedlayout height="32">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>410</width>
					<height>32</height>
					<visible>Control.HasFocus(120)</visible>
					<colordiffuse>object-color</colordiffuse>
					<texture>line.png</texture>
				</control>
				<control type="label">
					<posx>0</posx>
					<posy>0</posy>
					<width>400</width>
					<height>32</height>
					<font>font12</font>
					<aligny>center</aligny>
					<textcolor>main-text</textcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
			</focusedlayout>
		</control>
		<control type="spincontrol" id="121">
			<description>Next page button</description>
			<posx>430</posx>
			<posy>455</posy>
			<subtype>page</subtype>
			<font>font12</font>
			<onleft>120</onleft>
			<onright>120</onright>
			<ondown>120</ondown>
			<onup>120</onup>
			<showonepage>false</showonepage>
			<include>contentpanelslide</include>
			<visible>Control.IsVisible(120)</visible>
		</control>
		<!-- media infos -->
		<control type="group">
			<control type="image">
				<description>cover image</description>
				<posx>20</posx>
				<posy>55</posy>
				<width>90</width>
				<height>90</height>
				<info>MusicPlayer.Cover</info>
				<colordiffuse>FFFFFFFF</colordiffuse>
				<aspectratio>keep</aspectratio>
				<animation effect="fade" time="1500">WindowOpen</animation>
			</control>
			<control type="label">
				<description>Artist label</description>
				<posx>110</posx>
				<posy>55</posy>
				<width>400</width>
				<info>MusicPlayer.Artist</info>
				<align>left</align>
				<font>font12</font>
			</control>
			<control type="label">
				<description>Title label</description>
				<width>500</width>
				<posx>110</posx>
				<posy>75</posy>
				<info>MusicPlayer.Title</info>
				<align>left</align>
				<font>font13</font>
			</control>
			<control type="label">
				<description>Album label</description>
				<width>500</width>
				<posx>110</posx>
				<posy>95</posy>
				<info>MusicPlayer.Album</info>
				<align>left</align>
				<font>font12</font>
			</control>
			<control type="label">
				<description>time label</description>
				<width>430</width>
				<posx>110</posx>
				<posy>125</posy>
				<label>$INFO[musicplayer.Time]$INFO[musicplayer.Duration, / ]</label>
				<align>left</align>
				<font>font10</font>
			</control>
			<control type="label" id="200">
				<description>Scraper Label</description>
				<posx>465</posx>
				<posy>55</posy>
				<width>150</width>
				<label>-</label>
				<align>right</align>
				<textcolor>main-text</textcolor>
				<disabledcolor>disabled-text</disabledcolor>
				<font>font12</font>
			</control>
			<control type="label">
				<description>PartyMode Enabled</description>
				<posx>465</posx>
				<posy>95</posy>
				<width>200</width>
				<label>589</label>
				<visible>MusicPartyMode.Enabled</visible>
				<align>right</align>
				<font>font10</font>
			</control>
			<control type="label">
				<description>Playlist Pos No</description>
				<posx>465</posx>
				<posy>125</posy>
				<width>200</width>
				<label>$INFO[musicplayer.Playlistposition,$LOCALIZE[554] ]$INFO[musicplayer.Playlistlength, / ]</label>
				<visible>!MusicPartyMode.Enabled</visible>
				<align>right</align>
				<font>font10</font>
			</control>
		</control>
	</controls>
</window>
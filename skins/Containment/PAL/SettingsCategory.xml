<window id="12">
	<defaultcontrol>2</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<controls>
		<include>Background</include>
		<include>Container</include>
		<control type="group">
			<include>media-slide</include>
			<control type="grouplist" id="3">
				<description>button area</description>
				<posx>5</posx>
				<posy>110</posy>
				<width>160</width>
				<height>456</height>
				<itemgap>-3</itemgap>
				<texturefocus>button.png</texturefocus>
				<texturenofocus>button.png</texturenofocus>
				<onleft>5</onleft>
				<onright>5</onright>
				<onup>3</onup>
				<ondown>3</ondown>
			</control>
		</control>
		<control type="button" id="10">
			<description>default category button</description>
			<posx>5</posx>
			<posy>110</posy>
			<label>224</label>
			<font>font13</font>
		</control>
		<control type="grouplist" id="5">
			<description>control area</description>
			<posx>200</posx>
			<posy>110</posy>
			<width>500</width>
			<height>456</height>
			<itemgap>0</itemgap>
			<onleft>3</onleft>
			<onright>3</onright>
			<onup>5</onup>
			<ondown>5</ondown>
		</control>
		<control type="button" id="7">
			<description>default button</description>
			<posx>200</posx>
			<posy>110</posy>
			<font>font13</font>
			<width>500</width>
			<height>30</height>
			<colordiffuse>object-color</colordiffuse>
			<focusedcolor>button-focus-text</focusedcolor>
			<textcolor>button-nofocus-text</textcolor>
			<texturefocus>line.png</texturefocus>
			<texturenofocus>-</texturenofocus>
		</control>
		<control type="radiobutton" id="8">
			<description>default radiobutton</description>
			<posx>200</posx>
			<posy>110</posy>
			<font>font13</font>
			<width>500</width>
			<height>30</height>
			<texturefocus>line.png</texturefocus>
			<texturenofocus>-</texturenofocus>
			<focusedcolor>button-focus-text</focusedcolor>
			<textcolor>button-nofocus-text</textcolor>
			<colordiffuse>object-color</colordiffuse>
			<textureradiofocus>checkbox-fo.png</textureradiofocus>
			<textureradionofocus>checkbox-nf.png</textureradionofocus>
		</control>
		<control type="spincontrolex" id="9">
			<description>default spincontrolex</description>
			<posx>200</posx>
			<posy>110</posy>
			<font>font13</font>
			<width>500</width>
			<height>30</height>
			<texturefocus>line.png</texturefocus>
			<texturenofocus>-</texturenofocus>
			<textureup>scroll-up.png</textureup>
			<texturedown>scroll-down.png</texturedown>
			<textureupfocus>scroll-up-focus.png</textureupfocus>
			<texturedownfocus>scroll-down-focus.png</texturedownfocus>
			<font>font13</font>
			<focusedcolor>button-focus-text</focusedcolor>
			<textcolor>button-nofocus-text</textcolor>
			<disabledcolor>disabled-text</disabledcolor>
			<aligny>center</aligny>
			<reverse>yes</reverse>
		</control>
		<include>Shutters</include>
	</controls>
</window>
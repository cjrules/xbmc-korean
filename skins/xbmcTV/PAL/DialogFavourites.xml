<window type="dialog" id="105">
	<defaultcontrol always="true">450</defaultcontrol>
	<animation effect="fade" time="400">WindowOpen</animation>
	<animation effect="fade" time="400">WindowClose</animation>

	<coordinates>
		<system>1</system>
		<posx>25</posx>
		<posy>20</posy>
	</coordinates>

	<controls>

		<control type="image">
			<description>top</description>
			<posx>-15</posx>
			<posy>-5</posy>
			<width>380</width>
			<height>30</height>
			<texture>context2-top.png</texture>
		</control>
		<control type="image">
			<description>middle</description>
			<posx>-15</posx>
			<posy>25</posy>
			<width>380</width>
			<height>350</height>
			<texture>context2-middle.png</texture>
		</control>
		<control type="image">
			<description>bottom</description>
			<posx>-15</posx>
			<posy>375</posy>
			<width>380</width>
			<height>30</height>
			<texture>context2-bottom.png</texture>
		</control>
		<control type="group">
			<control type="list" id="450">
				<posx>-1</posx>
				<posy>15</posy>
				<width>348</width>
				<height>360</height>
				<onleft>1</onleft>
				<onright>10</onright>
				<onup>450</onup>
				<ondown>450</ondown>
				<scrolltime>200</scrolltime>
				<itemlayout height="40">
					<control type="label">
						<posx>10</posx>
						<posy>-</posy>
						<width>330</width>
						<height>40</height>
						<font>font16</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>button-text</textcolor>
						<selectedcolor>listselectedtext</selectedcolor>
						<info>ListItem.Label</info>
					</control>
				</itemlayout>
				<focusedlayout height="40">
					<control type="image">
						<width>350</width>
						<height>40</height>
						<posx>0</posx>
						<posy>0</posy>
						<texture>default-button.png</texture>
					</control>
					<control type="label">
						<posx>10</posx>
						<posy>-</posy>
						<width>330</width>
						<height>40</height>
						<font>font16</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>button-text</textcolor>
						<selectedcolor>listselectedtext</selectedcolor>
						<info>ListItem.Label</info>
					</control>
				</focusedlayout>
			</control>
		</control>
	</controls>
</window>
	<include>Black-Panel2</include>
	<include>Menu-Time</include>

	<control id="2">
		<description>Title Label</description>
		<include>Window-Title</include>
		<visible>!Window.IsActive(131)</visible>
	</control>

	<control type="grouplist" id="3">
		<description>button area</description>
		<posx>58</posx>
		<posy>80</posy>
		<width>550</width>
		<height>30</height>
		<itemgap>0</itemgap>
		<orientation>horizontal</orientation>
		<scrollspeed>2</scrollspeed>
		<include>FX-Button</include>
	</control>
	<control type="image" id="4">
		<description>button gap</description>
		<width>350</width>
		<height>38</height>
		<texture>-</texture>
	</control>

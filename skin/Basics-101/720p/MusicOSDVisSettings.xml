<window id="121">
	<defaultcontrol>2</defaultcontrol>
	<coordinates>
		<system>1</system>
		<posx>240</posx>
		<posy>80</posy>
	</coordinates>
	<include>dialogeffect</include>
	<controls>
		<control type="image">
			<description>background image</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>800</width>
			<height>470</height>
			<colordiffuse>AAFFFFFF</colordiffuse>
			<texture border="6">dialog_background.png</texture>
		</control>
		<control type="image">
			<description>background image</description>
			<posx>5</posx>
			<posy>5</posy>
			<width>790</width>
			<height>460</height>
			<colordiffuse>BBFFFFFF</colordiffuse>
			<texture>menu_back.png</texture>
			<aspectratio>scale</aspectratio>
		</control>
		<control type="label" id="2">
			<description>heading label</description>
			<posx>20</posx>
			<posy>10</posy>
			<width>760</width>
			<align>left</align>
			<font>font16</font>
			<textcolor>red</textcolor>
		</control>
		<control type="label" id="3">
			<description>No Presets label</description>
			<posx>20</posx>
			<posy>200</posy>
			<width>760</width>
			<label>13389</label>
			<align>center</align>
			<font>font16</font>
			<textcolor>white</textcolor>
		</control>
		<control type="group">
			<posx>755</posx>
			<posy>50</posy>
			<control type="button">
				<posx>0</posx>
				<posy>0</posy>
				<width>30</width>
				<height>30</height>
				<onclick>PageUp(60)</onclick>
				<texturefocus>scrollbar_arrow_up_focus.png</texturefocus>
				<texturenofocus>scrollbar_arrow_up.png</texturenofocus>
				<visible>Control.IsVisible(60)</visible>
			</control>
			<control type="scrollbar" id="60">
				<posx>0</posx>
				<posy>35</posy>
				<width>30</width>
				<height>330</height>
				<texturesliderbackground>button-nofocus.png</texturesliderbackground>
				<texturesliderbar>scrollbar_bar.png</texturesliderbar>
				<texturesliderbarfocus>scrollbar_bar_focus.png</texturesliderbarfocus>
				<textureslidernib>-</textureslidernib>
				<textureslidernibfocus>-</textureslidernibfocus>
				<onleft>5</onleft>
				<onright>5</onright>
				<showonepage>false</showonepage>
				<orientation>vertical</orientation>
			</control>
			<control type="button">
				<posx>0</posx>
				<posy>370</posy>
				<width>30</width>
				<height>30</height>
				<onclick>PageDown(60)</onclick>
				<texturefocus>scrollbar_arrow_down_focus.png</texturefocus>
				<texturenofocus>scrollbar_arrow_down.png</texturenofocus>
				<visible>Control.IsVisible(60)</visible>
			</control>
		</control>
		<control type="grouplist" id="5">
			<description>control area</description>
			<posx>15</posx>
			<posy>50</posy>
			<width>735</width>
			<height>400</height>
			<itemgap>5</itemgap>
			<pagecontrol>60</pagecontrol>
			<onleft>60</onleft>
			<onright>60</onright>
			<onup>5</onup>
			<ondown>5</ondown>
			<animation effect="slide" start="0,0" end="20,0" time="0" condition="!Control.IsVisible(60)">Conditional</animation>
		</control>
		<control type="button" id="7">
			<description>Default Button</description>
			<posx>0</posx>
			<posy>0</posy>
			<height>45</height>
			<font>font13</font>
			<textcolor>white</textcolor>
			<textoffsetx>7</textoffsetx>
			<texturefocus>button-focus.png</texturefocus>
			<texturenofocus>button-nofocus.png</texturenofocus>
		</control>
		<control type="radiobutton" id="8">
			<description>Default RadioButton</description>
			<posx>0</posx>
			<posy>0</posy>
			<height>45</height>
			<font>font13</font>
			<textcolor>white</textcolor>
			<texturefocus>button-focus.png</texturefocus>
			<texturenofocus>button-nofocus.png</texturenofocus>
			<textoffsetx>7</textoffsetx>
			<textureradiofocus>radiobutton-focus.png</textureradiofocus>
			<textureradionofocus>radiobutton-nofocus.png</textureradionofocus>
		</control>
		<control type="spincontrolex" id="9">
			<description>Default SpinControlex</description>
			<posx>0</posx>
			<posy>0</posy>
			<height>45</height>
			<texturefocus>button-focus.png</texturefocus>
			<texturenofocus>button-nofocus.png</texturenofocus>
			<textureup>scroll-up.png</textureup>
			<texturedown>scroll-down.png</texturedown>
			<textureupfocus>scroll-up-focus.png</textureupfocus>
			<texturedownfocus>scroll-down-focus.png</texturedownfocus>
			<font>font13</font>
			<textcolor>white</textcolor>
			<disabledcolor>disabled</disabledcolor>
			<textoffsetx>7</textoffsetx>
			<aligny>center</aligny>
			<reverse>yes</reverse>
		</control>
		<control type="image" id="11">
			<description>separator image</description>
			<width>350</width>
			<height>5</height>
			<colordiffuse>AAFFFFFF</colordiffuse>
			<texture>separator.png</texture>
		</control>
	</controls>
</window>
<window id="125">
	<defaultcontrol>2</defaultcontrol>
	<include>Fade_Dialog_OpenClose_Animation</include>
	<coordinates>
		<system>1</system>
		<posx>100</posx>
		<posy>115</posy>
	</coordinates>
	<controls>
		<control type="image">
			<posx>-300</posx>
			<posy>-300</posy>
			<width>1320</width>
			<height>1176</height>
			<texture>black.png</texture>
		</control>
		<control type="image">
			<description>Dialog Background</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>520</width>
			<height>325</height>
			<texture border="0,35,0,10">Dialog.png</texture>
		</control>
		<control type="label">
			<description>Title Label</description>
			<posx>60</posx>
			<posy>0</posy>
			<width>400</width>
			<align>center</align>
			<label>13398</label>
			<font>font22</font>
		</control>
		<control type="fixedlist" id="11">
			<posx>-290</posx>
			<posy>35</posy>
			<width>1010</width>
			<height>406</height>
			<onleft>11</onleft>
			<onright>11</onright>
			<onup>2</onup>
			<ondown>2</ondown>
<!--			<pagecontrol>90</pagecontrol>  -->
			<focusposition>2</focusposition>
			<orientation>horizontal</orientation>
			<itemlayout height="200" width="220">
				<control type="image">
					<posx>40</posx>
					<posy>40</posy>
					<width>140</width>
					<height>140</height>
					<aspectratio aligny="bottom">keep</aspectratio>
					<info>ListItem.Icon</info>
				</control>
				<control type="image">
					<posx>40</posx>
					<posy>180</posy>
					<width>140</width>
					<height>140</height>
					<texture flipy="true" diffuse="diffuse_mirror.png" /> 
					<aspectratio aligny="top">keep</aspectratio>
					<info>ListItem.Icon</info>
				</control>
				<control type="label">
					<posx>110</posx>
					<posy>190</posy>
					<width>200</width>
					<height>22</height>
					<font>font12</font>
					<align>center</align>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			<focusedlayout height="300"  width="220">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>220</width>
					<height>225</height>
					<texture>folder-focus.png</texture>
					<animation effect="zoom" start="20,25,180,180" end="0,0,220,225" time="150">focus</animation>
				</control>
				<control type="image">
					<posx>10</posx>
					<posy>10</posy>
					<width>200</width>
					<height>205</height>
					<aspectratio>keep</aspectratio>
					<info>ListItem.Icon</info>
					<animation effect="zoom" start="40,40,140,140" end="10,10,200,205" time="150">focus</animation>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>225</posy>
					<width>220</width>
					<height>220</height>
					<texture flipy="true" diffuse="diffuse_mirror.png">folder-focus.png</texture>
					<animation effect="zoom" start="20,180,180,180" end="0,225,220,225" time="150">focus</animation>
				</control>
				<control type="image">
					<posx>10</posx>
					<posy>245</posy>
					<width>200</width>
					<height>205</height>
					<texture flipy="true" diffuse="diffuse_mirror.png" /> 
					<info>ListItem.Icon</info>
					<animation effect="zoom" start="40,205,140,140" end="10,245,200,205" time="150">focus</animation>
				</control>
				<control type="label">
					<posx>110</posx>
					<posy>230</posy>
					<width>220</width>
					<height>16</height>
					<font>font14</font>
					<align>center</align>
					<scroll>true</scroll>
					<info>ListItem.Label</info>
				</control>
			</focusedlayout>
		</control>
		<control type="button" id="2">
			<description>Create Bookmark button</description>
			<posx>50</posx>
			<posy>285</posy>
			<height>25</height>
			<width>200</width>
			<align>center</align>
			<aligny>center</aligny>
			<label>294</label>
			<texturefocus>Dialog_Button_Focus.png</texturefocus>
			<texturenofocus>Dialog_Button_NoFocus.png</texturenofocus>
			<onup>11</onup>
			<onleft>3</onleft>
			<onright>3</onright>
			<ondown>11</ondown>
		</control>
		<control type="button" id="3">
			<description>Clear Bookmarks button</description>
			<posx>250</posx>
			<posy>285</posy>
			<height>25</height>
			<width>200</width>
			<align>center</align>
			<aligny>center</aligny>
			<label>296</label>
			<texturefocus>Dialog_Button_Focus.png</texturefocus>
			<texturenofocus>Dialog_Button_NoFocus.png</texturenofocus>
			<onup>11</onup>
			<onleft>2</onleft>
			<onright>2</onright>
			<ondown>11</ondown>
		</control>
	</controls>
</window>
<window>
	<id>2006</id>
	<defaultcontrol>2</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<controls>

	<control>
		<description>visualisation</description>
		<type>visualisation</type>
		<id>2</id>
		<posx>0</posx>
		<posy>0</posy>
		<width>720</width>
		<height>576</height>
	</control>

	<!-- Codec & Viz Infos -->
	
	
	
	
	<control>
		<description>Preset Background Image</description>
		<type>image</type>
		<posx>-10</posx>
		<posy>40</posy>
		<width>740</width>
		<height>65</height>
		<texture>osd_menu_bg.png</texture>
		<include>media-infopanel-effect</include>
		<visible>Player.ShowCodec</visible>
	</control>

	<control>
		<description>Codec Info</description>
		<type>label</type>
		<width>600</width>
		<posx>75</posx>
		<posy>50</posy>
		<label>Audio | Codec: $INFO[musicplayer.Codec] - Bitrate: $INFO[musicplayer.Bitrate]kbps - Bits: $INFO[musicplayer.bitspersample]bit - Freq.: $INFO[musicplayer.Samplerate]kHz - Channels: $INFO[musicplayer.Channels]</label>
		<align>left</align>
		<font>font12</font>
		<include>media-infopanel-effect</include>
		<visible>Player.ShowCodec</visible>
	</control>

	<control>
		<description>Presets Label</description>
		<type>label</type>
		<posx>75</posx>
		<posy>75</posy>
		<width>560</width>
		<label>Visual | fps: $INFO[System.FPS] - </label>
		<font>font12</font>
		<align>left</align>
		<include>media-infopanel-effect</include>
		<visible>Player.ShowCodec</visible>
	</control>

	<control>
		<description>Presets Label</description>
		<type>label</type>
		<posx>225</posx>
		<posy>75</posy>
		<width>420</width>
		<label>$INFO[Visualisation.Name] - $INFO[Visualisation.Preset]</label>
		<font>font12</font>
		<align>left</align>
		<include>media-infopanel-effect</include>
		<visible>Player.ShowCodec</visible>
	</control>

	<control>
		<description>Preset Locked Image</description>
		<type>image</type>
		<posx>210</posx>
		<posy>76</posy>
		<width>12</width>
		<height>15</height>
		<texture>arenaitem-private.png</texture>
		<include>media-infopanel-effect</include>
		<visible>Visualisation.Locked + Player.ShowCodec</visible>
	</control>

	<!-- Media Infos -->
<control>
<description>Media Info Background Image</description>
<type>image</type>
<id>1</id>
<posx>-10</posx>
<posy>445</posy>
<width>740</width>
<height>131</height>
<texture>OSD.png</texture>
<visible>Player.ShowInfo + Player.HasAudio</visible>
</control>

	<control type="group">
<posx>-50</posx>
<posy>20</posy>
	<control>
		<description>Cover Image</description>
		<type>image</type>
		<posx>65</posx>
		<posy>455</posy>
		<width>85</width>
		<height>100</height>
		<texture>case1.png</texture>
		<include>media-infopanel-effect</include>
		<visible>Player.ShowInfo + Player.HasAudio</visible>
	</control>
	
	<control>
		<description>Cover Image</description>
		<type>image</type>
		<posx>76</posx>
		<posy>462</posy>
		<width>69</width>
		<height>87</height>
		<info>MusicPlayer.Cover</info>
		<include>media-infopanel-effect</include>
		<visible>Player.ShowInfo + Player.HasAudio</visible>
	</control>

<control>
		<description>Cover Image</description>
		<type>image</type>
		<posx>64</posx>
		<posy>455</posy>
		<width>85</width>
		<height>100</height>
		<texture>case2.png</texture>
		<include>media-infopanel-effect</include>
		<visible>Player.ShowInfo + Player.HasAudio</visible>
	</control>

	<control>
		<description>Artist Label</description>
		<type>label</type>
		<width>400</width>
		<posx>157</posx>
		<posy>468</posy>
		<align>left</align>
		<font>Heading-M</font>
		<info>MusicPlayer.Artist</info>
		<include>media-infopanel-effect</include>
		<visible>Player.ShowInfo + Player.HasAudio</visible>
		</control>

	<control>
		<description>Title Label</description>
		<type>label</type>
		<width>400</width>
		<posx>157</posx>
		<posy>500</posy>
		<align>left</align>
		<font>fontKB</font>
		<info>MusicPlayer.Title</info>
		<include>media-infopanel-effect</include>
		<visible>Player.ShowInfo + Player.HasAudio</visible>
		</control>

	<control>
		<description>Album Label</description>
		<type>label</type>
		<width>430</width>
		<posx>157</posx>
		<posy>528</posy>
		<info>MusicPlayer.Album</info>
		<align>left</align>
		<font>fontKB</font>
		<include>media-infopanel-effect</include>
		<visible>Player.ShowInfo + Player.HasAudio</visible>
		</control>

	
	<control>
		<description>Time Label</description>
		<type>label</type>
		<width>200</width>
		<posx>660</posx>
		<posy>468</posy>
		<align>right</align>
		<font>Heading-M</font>
		<label>$INFO[System.Time]</label>
		<include>media-infopanel-effect</include>
		<visible>Player.ShowInfo</visible>
		</control>

		<control>
		<description>Time Label</description>
		<type>label</type>
		<width>200</width>
		<posx>660</posx>
		<posy>500</posy>
		<align>right</align>
		<font>fontKB</font>
		<label>$INFO[musicplayer.Time] / $INFO[musicplayer.Duration]</label>
		<include>media-infopanel-effect</include>
		<visible>Player.ShowInfo + Player.HasAudio</visible>
		</control>

		<control>
		<description>Playlist Label</description>
		<type>label</type>
		<width>200</width>
		<posx>660</posx>
		<posy>528</posy>
		<align>right</align>
		<font>fontKB</font>
		<label>$LOCALIZE[554] $INFO[musicplayer.Playlistposition] / $INFO[musicplayer.Playlistlength]</label>
		<include>media-infopanel-effect</include>
		<visible>Player.ShowInfo + Player.HasAudio</visible>
		</control>

</control>

	</controls>
</window>
			<height>40</height>
			<align>center</align>
			<label>222</label>
			<font>font13</font>
			<texturefocus>folder-focus2.png</texturefocus>
			<texturenofocus>dialog-button-nofocus.png</texturenofocus>
			<onleft>28</onleft>
			<onright>28</onright>
			<onup>5</onup>

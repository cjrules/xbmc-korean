<window id="127">
	<defaultcontrol>5</defaultcontrol>
	<coordinates>
		<system>1</system>
		<posx>40</posx>
		<posy>100</posy>
		<origin x="40" y="125">!Control.IsVisible(5)</origin>
	</coordinates>
	<include>dialogeffect</include>
	<controls>
		<control type="image">
			<posx>0</posx>
			<posy>0</posy>
			<width>1200</width>
			<height>500</height>
			<texture border="0,45,0,10">Dialog.png</texture>
		</control>
		<control type="image">
			<posx>580</posx>
			<posy>52</posy>
			<width>40</width>
			<height>16</height>
			<texture>Arrow_Up.png</texture>
			<visible>Container(3).HasPrevious</visible>
		</control>
		<control type="image">
			<posx>580</posx>
			<posy>420</posy>
			<width>40</width>
			<height>16</height>
			<texture>Arrow_Down.png</texture>
			<visible>Container(3).HasNext</visible>
		</control>
		<control type="label" id="2">
			<description>heading label</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>1200</width>
			<height>40</height>
			<align>center</align>
			<aligny>center</aligny>
			<font>font16</font>
			<label>-</label>
			<textcolor>red</textcolor>
			<shadowcolor>black</shadowcolor>
		</control>
		<control type="grouplist" id="5">
			<description>control area</description>
			<posx>220</posx>
			<posy>55</posy>
			<width>760</width>
			<height>350</height>
			<itemgap>5</itemgap>
			<pagecontrol>60</pagecontrol>
			<onleft>28</onleft>
			<onright>28</onright>
			<onup>5</onup>
			<ondown>5</ondown>
			<animation effect="slide" start="0,0" end="20,0" time="0" condition="!Control.IsVisible(60)">Conditional</animation>
		</control>
		<control type="button" id="7">
			<description>Default Button</description>
			<posx>0</posx>
			<posy>0</posy>
			<height>45</height>
			<font>font13</font>
			<textcolor>white</textcolor>
			<textoffsetx>7</textoffsetx>
			<texturefocus border="10,0,10,0">folder-focus2.png</texturefocus>
			<texturenofocus>dialog-button-nofocus.png</texturenofocus>
		</control>
		<control type="radiobutton" id="8">
			<description>Default RadioButton</description>
			<posx>0</posx>
			<posy>0</posy>
			<height>45</height>
			<font>font13</font>
			<textcolor>white</textcolor>
			<texturefocus border="10,0,10,0">folder-focus2.png</texturefocus>
			<texturenofocus>dialog-button-nofocus.png</texturenofocus>
			<textoffsetx>7</textoffsetx>
			<textureradiofocus>radiobutton-focus.png</textureradiofocus>
			<textureradionofocus>radiobutton-nofocus.png</textureradionofocus>
		</control>
		<control type="spincontrolex" id="9">
			<description>Default SpinControlex</description>
			<posx>0</posx>
			<posy>0</posy>
			<height>45</height>
			<texturefocus border="10,0,10,0">folder-focus2.png</texturefocus>
			<texturenofocus>dialog-button-nofocus.png</texturenofocus>
			<textureup>scroll-up.png</textureup>
			<texturedown>scroll-down.png</texturedown>
			<textureupfocus>scroll-up-focus.png</textureupfocus>
			<texturedownfocus>scroll-down-focus.png</texturedownfocus>
			<font>font13</font>
			<textcolor>white</textcolor>
			<disabledcolor>disabled</disabledcolor>
			<textoffsetx>7</textoffsetx>
			<aligny>center</aligny>
			<reverse>yes</reverse>
		</control>

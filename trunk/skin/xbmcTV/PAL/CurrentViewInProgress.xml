					<height>20</height>
					<font>font12</font>
					<align>left</align>
					<scroll>true</scroll>
					<textcolor>black-text</textcolor>
					<selectedcolor>listselectedtext</selectedcolor>
					<info>ListItem.Label</info>
					<visible>!Skin.HasSetting(widelabels)</visible>
				</control>
			</focusedlayout>
		</control>

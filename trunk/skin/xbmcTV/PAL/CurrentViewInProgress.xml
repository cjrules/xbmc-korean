			<posx>0</posx>
			<posy>0</posy>
			<width>700</width>
			<height>565</height>
			<onleft>2</onleft>
			<onright>24</onright>
			<onup>55</onup>
			<ondown>55</ondown>
			<scrolltime>200</scrolltime>
			<pagecontrol>24</pagecontrol>
			<include></include>
			<itemlayout height="94" width="350">
				<control type="image">
					<posx>5</posx>
					<posy>2</posy>

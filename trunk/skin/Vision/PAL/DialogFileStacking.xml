<window id="2008">
	<defaultcontrol>450</defaultcontrol>
	<animation effect="slide" time="150" start="0,-150" condition="!Skin.HasSetting(animated)">WindowOpen</animation>
	<animation effect="slide" time="150" end="0,-150" condition="!Skin.HasSetting(animated)">WindowClose</animation>
	<controls>
		<control type="image">
			<description>popup music scan dialog background</description>
			<posx>100</posx>
			<posy>5</posy>
			<width>720</width>
			<height>80</height>
			<texture>ListItem_Focus.png</texture>
		</control>
		<control type="fixedlist" id="450">
			<posx>100</posx>
			<posy>25</posy>
			<width>620</width>
			<height>100</height>
			<onleft>450</onleft>
			<onright>450</onright>
			<onup>450</onup>
			<ondown>450</ondown>
			<orientation>horizontal</orientation>
			<focusposition>12</focusposition>
			<viewtype label="535">list</viewtype>
			<scrolltime>200</scrolltime>
			<itemlayout width="30">
				<control type="image">
					<posx>0</posx>
					<posy>5</posy>
					<width>40</width>
					<height>40</height>
					<info>ListItem.Icon</info>
					<texture>button-cd-nofocus.png</texture>
				</control>
			</itemlayout>
			<focusedlayout width="40">
				<control type="image">
					<posx>5</posx>
					<posy>5</posy>
					<width>40</width>
					<height>40</height>
					<info>ListItem.Icon</info>
					<texture>button-cd-focus.png</texture>
					<animation effect="zoom" end="0,0,50,50" time="100">focus</animation>
				</control>
				<control type="label">
					<posx>50</posx>
					<posy>30</posy>
					<width>60</width>
					<height>22</height>
					<font>font16</font>
					<align>left</align>
					<info>ListItem.Label</info>
					<include>shadow-black</include>
				</control>
			</focusedlayout>
		</control>
		<control type="label">
			<posx>680</posx>
			<posy>5</posy>
			<height>22</height>
			<font>font20</font>
			<align>right</align>
			<info>ListItem.Label</info>
			<include>shadow-black</include>
		</control>
	</controls>
</window>
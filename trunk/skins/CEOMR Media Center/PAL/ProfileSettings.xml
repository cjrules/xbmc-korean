<window id="130">

  <defaultcontrol>1</defaultcontrol>

  <animation effect="fade" time="200">WindowOpen</animation>
  <animation effect="fade" time="200">WindowClose</animation>

  <coordinates>
    <system>1</system>
    <posx>155</posx>
    <posy>132</posy>
  </coordinates>
   <visible>Window.IsActive(130)</visible>
  <controls>

    <control type="image" id="0">
      <posx>-300</posx>
      <posy>-300</posy>
      <width>1300</width>
      <height>1300</height>
      <texture>background_dialog.png</texture>
      <visible>![Window.IsActive(29)]</visible>
      <animation effect="fade" time="2000">WindowOpen</animation>
    </control>

    <control type="image" id="1">
      <description>background image</description>
      <posx>-20</posx>
      <posy>-30</posy>
      <width>446</width>
      <height>430</height>
      <texture>osd_menu_bg.png</texture>
    </control>

    <control type="image" id="2">
      <description>profile image</description>
      <posx>15</posx>
      <posy>17</posy>
      <width>80</width>
      <height>80</height>
      <align>left</align>
      <font>font13</font>
      <texture>user.png</texture>
    </control>

    <control type="label" id="0">
      <description>profile name label</description>
      <posx>140</posx>
      <posy>6</posy>
      <width>280</width>
      <align>left</align>
      <font>font13</font>
      <textcolor>DDced8da</textcolor>
      <label>$LOCALIZE[ 20093 ]</label>
   </control>
  

    <control type="label" id="1000">
      <description>profile name label</description>
      <posx>140</posx>
      <posy>26</posy>
      <width>280</width>
      <align>left</align>
      <font>font13</font>
      <textcolor>FFFFFFFF</textcolor>
    </control>

    <control type="label" id="0">
      <description>profile directory label</description>
      <posx>140</posx>
      <posy>52</posy>
      <width>280</width>
      <align>left</align>
      <font>font13</font>
      <textcolor>DDced8da</textcolor>
      <label>$LOCALIZE[ 20070 ]</label>
   </control>


    <control type="label" id="1001">
      <description>profile directory label</description>
      <posx>140</posx>
      <posy>78</posy>
      <width>280</width>
      <align>left</align>
      <font>font13</font>
      <textcolor>FFFFFFFF</textcolor>
    </control>
    
    <control type="spincontrol" id="60">
      <description>Next page button</description>
      <posx>371</posx>
      <posy>8</posy>
      <subtype>page</subtype>
      <font>font12</font>
	<onleft>5</onleft>
	<onright>28</onright>
	<ondown>5</ondown>
	<onup>28</onup>
      <visible>!Window.IsActive(131)</visible>
    </control>

    <control type="grouplist" id="5">
      <description>control area</description>
      <posx>0</posx>
      <posy>135</posy>
      <width>408</width>
      <height>175</height>
	<itemgap>0</itemgap>
	<pagecontrol>60</pagecontrol>
      <visible>!Window.IsActive(131)</visible>
	<onleft>28</onleft>
	<onright>60</onright>
	<onup>28</onup>
	<ondown>28</ondown>
    </control>

    <control type="image" id="6">
      <description>control gap</description>
      <width>350</width>
      <height>31</height>
      <texture>-</texture>
      <visible>!Window.IsActive(131)</visible>
    </control>

    <control type="button" id="7">
      <description>Default Button</description>
      <posx>520</posx>
      <posy>150</posy>
      <height>29</height>
      <font>font12</font>
      <textoffsetx>12</textoffsetx>
      <aligny>center</aligny>
      <texturefocus>list-focus.png</texturefocus>
      <texturenofocus>-</texturenofocus>
      <visible>!Window.IsActive(131)</visible>
    </control>

    <control type="radiobutton" id="8">
      <description>Default RadioButton</description>
      <posx>530</posx>
      <posy>150</posy>
      <height>31</height>
      <font>font12</font>
      <texturefocus>list-focus.png</texturefocus>
      <texturenofocus>-</texturenofocus>
      <textoffsetx>12</textoffsetx>
      <aligny>center</aligny>
      <textureradiofocus>settings-check-box.png</textureradiofocus>
      <textureradionofocus>settings-check-boxNF.png</textureradionofocus>
      <visible>!Window.IsActive(131)</visible>
    </control>

    <control type="spincontrolex" id="9">
      <description>Default SpinControlex</description>
      <posx>530</posx>
      <posy>150</posy>
      <height>33</height>
      <width>300</width>
      <texturefocus>list-focus.png</texturefocus>
      <texturenofocus>-</texturenofocus>
      <textureup>scroll-up.png</textureup>
      <texturedown>scroll-down.png</texturedown>
      <textureupfocus>scroll-up-focus.png</textureupfocus>
      <texturedownfocus>scroll-down-focus.png</texturedownfocus>
      <font>font13</font>
      <include>textcolorwhite</include>
      <disabledcolor>60d0d0d0</disabledcolor>
      <textoffsetx>7</textoffsetx>
      <aligny>center</aligny>
      <reverse>yes</reverse>
      <visible>!Window.IsActive(131)</visible>
    </control>

    <control type="sliderex" id="10">
      <description>Default Slider</description>
      <posx>1530</posx>
      <posy>150</posy>
      <height>31</height>
      <width>300</width>
      <texturefocus>list-focus.png</texturefocus>
      <texturenofocus>-</texturenofocus>
      <sliderwidth>100</sliderwidth>
      <sliderheight>20</sliderheight>
      <texturesliderbar>osd_slider_bg.png</texturesliderbar>
      <textureslidernib>osd_slider_nibNF.png</textureslidernib>
      <textureslidernibfocus>osd_slider_nibFO.png</textureslidernibfocus>
      <font>font12</font>
      <include>textcolorwhite</include>
      <disabledcolor>60d0d0d0</disabledcolor>
      <textoffsetx>12</textoffsetx>
      <visible>!Window.IsActive(131)</visible>
    </control>

    <control type="image" id="11">
      <description>separator image</description>
      <width>350</width>
      <height>5</height>
      <texture>separatorOSD.png</texture>
      <visible>!Window.IsActive(131)</visible>
    </control>

    <control type="button" id="28">
       <description>OK</description>
       <posx>220</posx>
       <posy>337</posy>
       <width>90</width>
       <height>32</height>
       <label>186</label>
       <onleft>29</onleft>
       <onright>29</onright>
       <onup>5</onup>
       <ondown>5</ondown>
       <font>font13</font>
    </control>
    
    <control type="button" id="29">
       <description>cancel</description>
       <posx>315</posx>
       <posy>337</posy>
       <width>90</width>
       <height>32</height>
       <label>222</label>
       <onleft>28</onleft>
       <onright>28</onright>
       <onup>5</onup>
       <ondown>5</ondown>
       <font>font13</font>
    </control>

  </controls>

</window>
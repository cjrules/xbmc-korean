<window id="103">

  <defaultcontrol always="true">65</defaultcontrol>

  <animation effect="fade" time="200">WindowOpen</animation>
  <animation effect="fade" time="200">WindowClose</animation>

  <controls>

     <control type="image" id="1">
        <posx>0</posx>
        <posy>0</posy>
      <width>720</width>
      <height>576</height>
      <texture>background_dialog.png</texture>
      <visible>!Window.IsActive(109)</visible>
    </control>

     <control type="image" id="1">
      <description>background image</description>
        <posx>0</posx>
        <posy>0</posy>
      <width>720</width>
      <height>576</height>
      <texture>background.png</texture>
      <visible>!Skin.HasSetting(hintergrund)+Window.IsActive(0)</visible>
    </control>

     <control type="image" id="1">
      <description>background self</description>
        <posx>0</posx>
        <posy>0</posy>
      <width>720</width>
      <height>576</height>
      <visible>Skin.HasSetting(hintergrund)+Window.IsActive(0)</visible>
      <info>Skin.String(hintergrund)</info>
    </control>

    <control type="image" id="1">
      <description>xbmc logo</description>
      <include>xbmclogo</include>
      <visible>!Skin.HasSetting(logo)+Window.IsActive(0)</visible>
      <animation effect="zoom" time="600" start="150" end="150">WindowOpen</animation>
      <animation effect="zoom" time="600" start="150" end="150">WindowClose</animation>

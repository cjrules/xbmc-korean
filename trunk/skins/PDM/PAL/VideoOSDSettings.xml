<window type="123">
	<defaultcontrol>2</defaultcontrol>
	<include>dialog-effect</include> 
	<coordinates>
	<system>1</system>
	<origin x="150" y="110">true</origin>
	</coordinates>
	<controls>

	<control type="image">
		<description>Background Image</description>
		<posx>0</posx>
		<posy>25</posy>
		<width>420</width>
		<height>320</height>
		<texture>osd_menu_bg.png</texture>
	</control>

	<control type="image">
		<description>Background Header Image</description>
		<posx>0</posx>
		<posy>-5</posy>
		<width>420</width>
		<height>31</height>
		<texture>osd_settings_header.png</texture>
	</control>

	<control type="label" id="2">
		<description>Settings Label</description>
		<posx>0</posx>
		<posy>2</posy>
		<width>420</width>
		<align>center</align>
		<font>font12</font>
	</control>

	<control type="label" id="3">
		<description>No Settings Label</description>
		<posx>0</posx>
		<posy>170</posy>
		<width>420</width>
		<align>center</align>
		<label>13390</label>
	</control>

	<control type="spincontrol" id="60">
		<description>Next Page Button</description>
		<posx>360</posx>
		<posy>330</posy>
		<subtype>int</subtype>
		<font>font10</font>
		<onleft>60</onleft>
		<onright>60</onright>
		<ondown>2</ondown>
	</control>

	<control type="image" id="5">
		<description>Control Area</description>
		<posx>5</posx>
		<posy>30</posy>
		<width>408</width>
		<height>300</height>
		<texture>-</texture>
	</control>

	<control type="image" id="6">
		<description>Control Gap</description>
		<width>350</width>
		<height>33</height>
		<texture>-</texture>
	</control>

	<control type="button" id="7">
		<description>Default Button</description>
		<posx>530</posx>
		<posy>150</posy>
		<height>31</height>
		<font>font12</font>
		<textoffsetx>12</textoffsetx>
		<aligny>center</aligny>
		<texturefocus>list-focus.png</texturefocus>
		<texturenofocus>-</texturenofocus>
	</control>

	<control type="radiobutton" id="8">
		<description>Default RadioButton</description>
		<posx>530</posx>
		<posy>150</posy>
		<height>31</height>
		<font>font12</font>
		<texturefocus>list-focus.png</texturefocus>
		<texturenofocus>-</texturenofocus>
		<textoffsetx>12</textoffsetx>
		<aligny>center</aligny>
		<textureradiofocus>settings-check-box.png</textureradiofocus>
		<textureradionofocus>settings-check-boxNF.png</textureradionofocus>
	</control>

	<control type="spincontrolex" id="9">
		<description>Default SpinControlex</description>
		<posx>530</posx>
		<posy>150</posy>
		<height>31</height>
		<width>300</width>
		<texturefocus>list-focus.png</texturefocus>
		<texturenofocus>-</texturenofocus>
		<textureup>scroll-up.png</textureup>
		<texturedown>scroll-down.png</texturedown>
		<textureupFocus>scroll-up-focus.png</textureupFocus>
		<texturedownFocus>scroll-down-focus.png</texturedownFocus>
		<font>font10</font>
		<textcolor>ffffffff</textcolor>
		<colordiffuse>ffffffff</colordiffuse>
		<disabledcolor>60d0d0d0</disabledcolor>
		<textoffsetx>12</textoffsetx>
		<aligny>center</aligny>
		<reverse>yes</reverse>
	</control>

	<control type="sliderex" id="10">
		<description>Default Slider</description>
		<posx>530</posx>
		<posy>150</posy>
		<height>31</height>
		<width>300</width>
		<texturefocus>list-focus.png</texturefocus>
		<texturenofocus>-</texturenofocus>
		<sliderwidth>100</sliderwidth>
		<sliderheight>20</sliderheight>
		<texturesliderbar>osd_slider_bg.png</texturesliderbar>
		<textureslidernib>osd_slider_nibNF.png</textureslidernib>
		<textureslidernibfocus>osd_slider_nibFO.png</textureslidernibfocus>

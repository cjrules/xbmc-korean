<window id="3">
	<defaultcontrol>20</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<controls>
		<include>Background</include>
<!-- Start List Containers -->

		<control type="image">
			<description>panel</description>
			<posx>-400</posx>
			<posy>0</posy>
			<height>576</height>
			<width>400</width>
			<texture>dialog-container.png</texture>
			<animation effect="slide" end="360,0" time="225">WindowOpen</animation>
			<animation effect="slide" start="360,0" time="225">WindowClose</animation>
		</control>

		<control type="image">
			<description>panel</description>
			<posx>720</posx>
			<posy>0</posy>
			<height>576</height>
			<width>400</width>
			<texture flipx="true">dialog-container.png</texture>
			<animation effect="slide" end="-360,0" time="225">WindowOpen</animation>
			<animation effect="slide" start="-360,0" time="225">WindowClose</animation>
		</control>

<!-- End List Containers -->

